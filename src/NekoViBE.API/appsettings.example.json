{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost,1433;Database=NekoViDb;User Id=sa;Password=YourStrong@Password;TrustServerCertificate=True;",
    "OuterDbConnection": "Server=localhost,1433;Database=NekoViOuterDb;User Id=sa;Password=YourStrong@Password;TrustServerCertificate=True;"
  },
  "Jwt": {
    "Key": "REPLACE_WITH_SECURE_RANDOM_KEY",
    "Issuer": "NekoViBEApp",
    "Audience": "NekoViClients",
    "ExpireInMinutes": 60,
    "RefreshTokenExpireInDays": 7
  },
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:3000",
      "http://localhost:3001",
      "http://localhost:5173",
      "http://localhost:4200",
      "https://your-cms-domain.vercel.app",
      "https://your-customer-website.vercel.app"
    ]
  },
  "AdminUser": {
    "Email": "admin@nekovi.com",
    "DefaultPassword": "CHANGE_ME_Strong@Password1"
  },
  "DataSeeding": {
    "EnableSeeding": true
  },
  "BaseUrl": "https://your-public-base-url.example",
  "VnPay": {
    "TmnCode": "YOUR_TMN_CODE",
    "HashSecret": "YOUR_HASH_SECRET",
    "ReturnUrl": "https://your-customer-website.vercel.app/vnpay/return",
    "BaseUrl": "https://sandbox.vnpayment.vn/paymentv2/vpcpay.html",
    "IpnUrl": "https://your-public-base-url.example/api/payment-callback/vnpay/IPN",
    "Version": "2.1.0",
    "OrderType": "other",
    "TimeOut": 900,
    "CardNumber": "9704xxxxxxxxxxxx",
    "CardOwner": "YOUR NAME",
    "CardPublishDate": "MM/YY",
    "CardBank": "NCB",
    "OTP": "123456"
  },
  "MoMo": {
    "MomoPartnerCode": "YOUR_MOMO_PARTNER_CODE",
    "MomoPartnerName": "NekoVi",
    "MomoStoreId": "YOUR_STORE_ID",
    "MomoAccessToken": "YOUR_MOMO_ACCESS_TOKEN",
    "MomoSecretKey": "YOUR_MOMO_SECRET_KEY",
    "MomoApiEndpoint": "https://test-payment.momo.vn/v2/gateway/api",
    "MomoIpnUrl": "https://your-public-base-url.example/api/payment-callback/momo/ipn",
    "MomoRedirectUrl": "https://your-customer-website.vercel.app/momo/result",
    "MomoMobileRedirectUrl": "nekovi://momo/result",
    "MomoIpnWhitelist": "118.69.208.0/20,210.245.113.71,127.0.0.1,::1"
  },
  "PasswordEncryptKey": "REPLACE_WITH_SECURE_RANDOM_KEY_256_BITS",
  "OTPSettings": {
    "Length": 6,
    "ExpirationMinutes": 5,
    "MaxAttempts": 3,
    "RateLimiting": {
      "Registration": {
        "CooldownMinutes": 1,
        "MaxRequestsPerWindow": 5,
        "WindowMinutes": 10,
        "BlockDurationMinutes": 10
      },
      "PasswordReset": {
        "CooldownMinutes": 1,
        "MaxRequestsPerWindow": 3,
        "WindowMinutes": 5,
        "BlockDurationMinutes": 15
      }
    }
  },
  "GoogleSettings": {
    "ClientId": "YOUR_GOOGLE_CLIENT_ID.apps.googleusercontent.com",
    "ClientSecret": "YOUR_GOOGLE_CLIENT_SECRET",
    "RedirectUri": "https://your-public-base-url.example/api/oauth/callback"
  },
  "AppSettings": {
    "AppName": "NekoVi",
    "SupportEmail": "support@your-domain.com"
  },
  "EmailSettings": {
    "SmtpServer": "smtp.gmail.com",
    "SmtpPort": 587,
    "SenderEmail": "your-email@gmail.com",
    "SenderName": "No Reply - NekoVi",
    "SenderPassword": "your-app-password",
    "EnableSsl": true
  },
  "Hangfire": {
    "UseOuterDatabase": true,
    "WorkerCount": 2,
    "HeartbeatInterval": 30,
    "QueuePollInterval": 15,
    "CommandBatchMaxTimeout": 300,
    "SlidingInvisibilityTimeout": 300,
    "UseRecommendedIsolationLevel": true,
    "DisableGlobalLocks": true,
    "Queues": [
      "default"
    ],
    "Retry": {
      "Attempts": 3,
      "DelayInSeconds": {
        "First": 60,
        "Second": 300,
        "Third": 600
      }
    },
    "Limits": {
      "MaxConcurrentJobsPerQueue": 2,
      "QueueTimeout": 1800
    },
    "Jobs": {},
    "Dashboard": {
      "Title": "NekoVi Token Management - Development",
      "StatsPollingInterval": 2000,
      "DefaultRecordsPerPage": 50,
      "DisplayStorageConnectionString": false,
      "EnableStatistics": true,
      "EnableMetrics": true,
      "Security": {
        "RequireAuthentication": false,
        "AllowAnonymous": true
      }
    }
  },
  "FileStorage": {
    "Type": "local",
    "BaseStorageUrl": "https://your-public-base-url.example"
  },
  "GHN": {
    "Token": "YOUR_GHN_TOKEN",
    "ClientId": "YOUR_GHN_CLIENT_ID",
    "ShopId": "YOUR_GHN_SHOP_ID",
    "BaseUrl": "https://dev-online-gateway.ghn.vn/shiip/public-api",
    "CreatePrefix": "/v2/shipping-order/create",
    "DetailPrefixByClientCode": "/v2/shipping-order/detail-by-client-code",
    "DatePrefix": "/v2/shift/date",
    "PreviewPrefix": "/v2/shipping-order/preview",
    "LeadTimePrefix": "/v2/shipping-order/leadtime",
    "UpdatePrefix": "/v2/shipping-order/update",
    "CancelPrefix": "/v2/switch-status/cancel",
    "ReturnPrefix": "/v2/switch-status/return",
    "GenTokenPrefix": "/v2/a5/gen-token",
    "DetailPrefix": "/v2/shipping-order/detail",
    "StoringPrefix": "/v2/switch-status/storing",
    "UpdateCODPrefix": "/v2/switch-status/updateCod",
    "GetStationPrefix": "/v2/station/get",
    "GetFeePrefix": "/v2/shipping-order/fee",
    "AvailableServicesPrefix": "/v2/shipping-order/available-services",
    "CallbackOrder": "https://your-public-base-url.example/api/shipping-order/ghn/callback",
    "ShopProvinceId": 202,
    "ShopDistrictId": 3695,
    "ShopWardCode": 90752
  }
}
